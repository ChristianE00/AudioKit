{
    "manifest_version": 3,
    "name": "Volume Adjuster and Equalizer",
    "version": "1.0",
    "description": "A simple volume adjuster and equalizer extension",
    "permissions": [
      "storage",
      "tabs",
      "tabCapture",
      "offscreen",
      "activeTab"
    ],
    "action": {
      "default_popup": "home.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "worker.js"
    }
  }